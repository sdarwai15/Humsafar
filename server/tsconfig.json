{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"pretty": true,
		"sourceMap": true,
		"target": "es6",
		"outDir": "./dist",
		"baseUrl": "./src",
		"noImplicitAny": true,
		"esModuleInterop": true
	},
	"include": ["./src/**/*.ts", ".env"],
	"exclude": ["node_modules"]
}